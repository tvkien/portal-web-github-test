@*<link rel="icon" type="image/png" sizes="192x192" href="/Content/images/android-chrome-192x192.png">*@
@if (ViewBag.PortalPageType == "Student")
{
    <text>
        <link rel="manifest" href="/Content/manifest/manifest_student.json">
    </text>
}
else if (ViewBag.PortalPageType == "Parent")
{
    <text>
        <link rel="manifest" href="/Content/manifest/manifest_parent.json">
    </text>
}
else
{
    <text>
        <link rel="manifest" href="/Content/manifest/manifest.json">
    </text>
}
<meta name="mobile-web-app-capable" content="yes">
<meta name="theme-color" content="#FFFFFF">
<meta name="apple-mobile-web-app-capable" content="yes">
<link rel="yandex-tableau-widget" href="/Content/images/yandex-browser-manifest.json">
<link rel="apple-touch-icon" href="~/Content/images/favicon/apple-touch-icon.png" sizes="180x180">
<link rel="icon" href="~/Content/images/favicon/favicon-32x32.png" sizes="32x32" type="image/png">
<link rel="icon" href="~/Content/images/favicon/favicon-16x16.png" sizes="16x16" type="image/png">
<link rel="manifest" href="~/Content/images/favicon/site.webmanifest">
<link rel="icon" href="~/Content/images/favicon/favicon.ico">
<meta name="msapplication-TileColor" content="#FFFFFF">
<meta name="msapplication-TileImage" content="/Content/images/mstile-144x144.png">
<meta name="msapplication-config" content="/Content/images/browserconfig.xml">
<meta property="twitter:image" content="/Content/images/twitter.png">
<meta property="og:image" content="/Content/images/open-graph.png">
