@model LinkIt.BubbleSheetPortal.Web.ViewModels.SendResultViewModel
<script src="@Url.Content("~/Content/themes/AssignmentRegrader/js/css_browser_selector.js")" type="text/javascript"></script>
@{
    ViewBag.Title = "Inbox";
    Layout = "~/Views/Shared/_LayoutParentConnect.cshtml";
}
@if (ViewBag.IsParent == true)
{
    @Html.Partial("ParentInbox", Model)
}
else
{
    @Html.Partial("StaffInbox", Model)
}


<script>
    $(function () {
        $('.parent_connect').addClass('current');
    });
</script>