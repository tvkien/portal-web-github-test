    .testPreferences__panel {
        margin-top: 15px;
        font-weight: 700;
        display: inline-block;
    }

    .btn__setting {
        background: #eee;
        color: #39c;
        cursor: pointer;
        border-radius: 6px 6px 0px 0px;
        padding: 10px 5px;
        font-size: 10pt;
        border-right: 1px solid #3399cc;
        width: 135px;
        text-align: center;
        display: block;
        float: left;
    }

        .btn__setting span {
            display: block;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            box-sizing: border-box;
        }

        .btn__setting:last-child {
            border-right: 0;
        }

    .sections__main {
        display: none;
        min-height: 645px;
    }

    .sections__wrapper--inner {
        min-height: 645px;
        position: relative;
    }

    .btn__setting.active__state {
