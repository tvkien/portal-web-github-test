@model LinkIt.BubbleSheetPortal.Web.ViewModels.SATReport.SATReportMasterViewModel
           
@if (Model.EssayComments.Any())
{
    <div style="font-size: 14px;">
        <div>
            @Html.Raw(Model.EssayCommentTitle)
        </div>    

        <br />
        <br />
        <table>
            <tr>
                @foreach (var essayComment in Model.EssayComments)
                {
                    <td style="width: 50%; padding: 0px 30px 0px 0px;">
                        @Html.Raw(essayComment)
                    </td>
                }
            </tr>
        </table>
    </div>
    <br/>
}


@if (!string.IsNullOrEmpty(Model.DistrictReportBannerUrl))
{
    <img src="@Model.DistrictReportBannerUrl" style="max-width: 960px;"/>
}



