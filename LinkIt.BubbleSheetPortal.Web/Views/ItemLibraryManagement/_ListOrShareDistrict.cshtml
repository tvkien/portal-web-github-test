@model int
<link href="@Url.Content("~/Content/themes/base/jquery.ui.dialog.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/css/dialog-draggable.css")" rel="stylesheet" type="text/css" />

<div id ="listDistrictDiv" />
<div id ="shareDistrictDiv" />

<script type="text/javascript">
    $(function () {
        init();
    });

    function init() {
        displayPublishedDistrict(@Model);
    }

    function displayPublishedDistrict(qtiBankId) {
        var jqListDistrictDiv = $('#listDistrictDiv');
        jqListDistrictDiv.empty();

        var param = { qTIBankId: qtiBankId };

        jqListDistrictDiv.load('@Url.Action("LoadListDistrictByQtiBank")', param, function (response) {
                if (response == false) {
                    alert("Error happen. Please refresh page.");
                }
            });
        }
</script>