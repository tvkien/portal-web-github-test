@model LinkIt.BubbleSheetPortal.Web.ViewModels.ObjectCommonCustomViewModel
<section >
    <div class='block-border'>
        <div class='block-content'>
            <h1>SGO Print</h1>             
            <div>
                <p>
                    Would you like to save it as a PDF?
                </p>
                <p >
                    <a id="idlinkSGOFilePrint" target="_blank" style="display: none;" href="#">
                        <u>
                            Click here to open PDF file
                        </u>
                    </a>
                </p>
                <p style="text-align: center;">
                    <button id="idbtnSGOSavePDFFile" type="button" onclick="printreportSGO(@Model.Id,'@Model.Name', @Model.ExtraId);">Save PDF</button>
                    <button id="idbtnCloseSGOPrintPopup" type="button" onclick="closeSGOPrintPopup();">Close</button>
                    <input id="hdfGenericPDFLoaded" type="hidden" value="0"/>
                </p>
            </div>
        </div>
    </div>
</section>
 