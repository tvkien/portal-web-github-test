@model LinkIt.BubbleSheetPortal.Web.ViewModels.PassThroughMessage


<textarea style="width: 800px; height: 200px;" id="xmlResponse" name="xmlResponse">@Model.MessageError</textarea>

<br/>
<input id="validate" value="Validate" type="button"/>

<script>
    $("#validate").click(function () {
        $.post('@Url.Action("ValidateSAMLToken","PassThrough")', { xmlResponse: $('#xmlResponse').val() }, function (response) {
            alert(response.Result);
        });
    });
</script>